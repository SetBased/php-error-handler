{
  "name": "setbased/error-handler",
  "description": "An error handler",
  "keywords": [
    "Error",
    "Handler"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "setbased/exception": "^2.3.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.5.28"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\ErrorHandler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\ErrorHandler\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin",
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
