  "keywords": [
    "Error",
    "Handler"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "setbased/exception": "^2.1"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
